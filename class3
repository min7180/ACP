#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void GetPrimeNumber(int* ran, int* prime, int* pn);

int main(void)
{
	int ran[20], prime[20];
	int pn = 0;
	int i = 0;

	GetPrimeNumber(ran, prime, &pn);
	for (; i < 20; i++)
		printf("%d\n", ran[i]);

	return 0;
}

void GetPrimeNumber(int* ran, int* prime, int* pn)
{
	int i, div;
	srand((unsigned int)time(NULL));

	for (i = 0; i < 20; i++)
	{
		ran[i] = (rand() % 100) + 1;
	}
	for (i = 0; i < 20; i++)
	{
		for (div = 2; div*div < (ran[i]) + 1; div++)
			if (ran[i] % div == 0) break;
		prime[(*pn)++] = ran[i];
	}

}
