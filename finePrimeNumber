#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void findPrimeNum(int* randNum);

int main(void)
{
	int i;
	int randNum[10];

	srand((unsigned int)time(NULL));
	for (i = 0; i < 10; i++)
	{
		randNum[i] = (rand() % 100 + 1);
		printf("randNume[%d]: %d\n", i + 1, randNum[i]);
	}

	printf("\n\n");
	findPrimeNum(randNum);
	printf("\n\n");
	system("pause");

	return 0;
}

void findPrimeNum(int* randNum)
{
	int i, j;
	int cnt;
	for (i = 0; i < 10; i++)
	{
		cnt = 0;
		for (j = 2; j*j < randNum[i] + 1; j++)
		{
			if (randNum[i] % j == 0)
			{
				cnt++;
				break;
			}
			else
				continue;
		}
		if (cnt == 0)
		{
			printf("PrimeNumber[%d]: %d\n", i + 1, randNum[i]);
		}
	}
}
